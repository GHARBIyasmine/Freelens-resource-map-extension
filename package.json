{
	"name": "@gharbi-yasmine/Freelens-resource-map",
	"version": "1.0.0",
	"description": "FreeLens extension to visualize Kubernetes resources and their relations",
	"homepage": "https://github.com/GHARBIyasmine/Freelens-resource-map-extension",
	"license": "MIT",
	"publisher": "Yasmine Gharbi",
	"main": "dist/main.js",
	"renderer": "dist/renderer.js",
	"engines": {
		"node": ">=20.0",
		"freelens": "^1.2.0"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"keywords": [
		"lens",
		"extension",
		"k8slens",
		"kubernetes",
		"freelensapp",
		"extension",
		"lens",
		"openlens",
		"freelens",
		"resource",
		"map"
	],
	"files": [
		"dist/"
	],
	"updateSource": "github|npmjs",
	"scripts": {
		"start": "webpack --watch",
		"build": "npm run clean && webpack",
		"clean": "rm -rf ./dist",
		"test": "jest"
	},
	"jest": {
		"runner": "@jest-runner/electron",
		"testEnvironment": "@jest-runner/electron/environment"
	},
	"contributes": {
		"crds": [],
		"cloudProviders": [],
		"kubernetesDistros": []
	},
	"overrides": {
		"react": "^17.0.2",
		"@types/react": "^17.0.85"
	},
	"devDependencies": {
		"@freelensapp/core": "^1.2.0",
		"@freelensapp/extensions": "^1.2.0",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"@types/d3-force": "^3.0.10",
		"@types/node": "^22.14.0",
		"@types/react": "^17.0.85",
		"@types/react-dom": "^17.0.26",
		"css-loader": "^6.11.0",
		"d3-force": "^3.0.0",
		"electron": "^35.0.3",
		"force-graph": "^1.49.5",
		"jest": "^29.7.0",
		"lint-staged": "^15.5.1",
		"mobx": "^6.10.2",
		"mobx-react": "^7.6.0",
		"prettier": "^3.5.3",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-force-graph-2d": "^1.27.1",
		"sass": "^1.86.3",
		"sass-loader": "^16.0.5",
		"semantic-release": "^24.2.3",
		"style-loader": "^4.0.0",
		"ts-loader": "^9.5.2",
		"type-fest": "^2.19.0",
		"typescript": "^5.8.2",
		"webpack": "^5.98.0",
		"webpack-cli": "^6.0.1"
	},
	"release": {
		"branches": [
			"master"
		],
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			"@semantic-release/changelog",
			"@semantic-release/npm",
			[
				"@semantic-release/github",
				{
					"assets": [
						{
							"path": "*.tgz",
							"name": "gharbi-yasmine-kube-resource-map-${nextRelease.version}.tgz"
						}
					]
				}
			],
			"@semantic-release/git"
		]
	},
	"lint-staged": {
		"**/*": "prettier --write --ignore-unknown"
	}
}
